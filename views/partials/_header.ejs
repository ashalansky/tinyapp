<header>
  <nav class="navbar navbar-expand-md navbar-light bg-warning">
    <a class="navbar-brand" href="/urls">TinyApp</a>
      <button class="navbar-toggler" type="button" data-toggle="collapse" data-target="#navbarNavAltMarkup" aria-controls="navbarNavAltMarkup" aria-expanded="false" aria-label="Toggle navigation">
      <span class="navbar-toggler-icon"></span>
    </button>
  <div class="collapse navbar-collapse" id="navbarNavAltMarkup">
    <div class="navbar-nav mr-auto">
      <a class="nav-item nav-link" href="/urls">My URLs</a>
      <a class="nav-item nav-link" href="/urls/new">Create New URL</a>
    </div>
    <div class= "navbar-nav mr">
    <div>
      <% if (userID) { %>
      <form action="/logout" method="POST">
        <label for="email">Logged In as: <%= userID.email %>!</label>
          <input type="text">
        <button type="submit" class="btn btn-outline-dark">Logout</button>
      </form>
        <% } else { %>
          <button class="btn btn-outline-dark" href="/login" type="submit">Login</button>
          <button class="btn btn-outline-dark" href="/registration" type="submit">Register</button>
        <% } %>
    </div>
  </div>
  </nav>
</header>